#include "s4_s905w2_ap201_1g.dts"
#include "coreelec_s4_common.dtsi"

/ {
	model = "Amlogic";
	coreelec-dt-id = "s4_s905w2_2g";

	/delete-node/ dvb-extern;
	/delete-node/ dvb-demux;
	/delete-node/ aml_dtv_demod;
	/delete-node/ adc_keypad;

	memory@00000000 {
		linux,usable-memory = <0x0 0x0 0x0 0x80000000>;
	};

	reserved-memory {
		logo_reserved:linux,meson-fb {
			alloc-ranges = <0x0 0x7f800000 0x0 0x800000>;
		};
	};

	gpio_keypad {
		status = "disabled";
	};
};

&fb {
	logo_addr = "0x7f800000";
};

&drm_vpu {
	logo_addr = "0x7f800000";
};

&sd_emmc_c {
	tx_delay = <0xa>;
};

&i2c1 {
	status = "disabled";
};
