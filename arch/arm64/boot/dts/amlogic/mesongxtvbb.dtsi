		};

		spicc_pins_y4y5y6: spicc_pins_y4y5y6{
			amlogic,setmask=<4 0x700000>;
			amlogic,clrmask=<4 0x708>;
			amlogic,pins="GPIOY_4","GPIOY_5","GPIOY_6";
