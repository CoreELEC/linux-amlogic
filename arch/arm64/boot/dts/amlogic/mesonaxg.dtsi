	i2c_slave:i2c_slave@ff806000{
		compatible = "amlogic, meson-i2c-slave";
		status = "disabled";
		reg = <0x0 0xff806000 0x0 0x20>;
		interrupts = <0 194 1>;
		pinctrl-names="default";
